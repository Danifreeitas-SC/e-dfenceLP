generator client {
    provider = "prisma-client-js"
    binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
    output = "/home/ubuntu/ed_fence_landing_page/nextjs_space/node_modules/.prisma/client"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model CallbackRequest {
  id        String   @id @default(cuid())
  name      String
  phone     String
  email     String?
  zipCode   String?
  fenceType String?
  message   String?
  campaign  String?  @default("general")
  status    String   @default("new")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
